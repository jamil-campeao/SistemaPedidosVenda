    object cdsCabecalhoPEDIDO_VALORTOTAL: TBCDField
      FieldName = 'PEDIDO_VALORTOTAL'
      DisplayFormat = '0.00'
      currency = True
      Precision = 14
      Size = 2
    end
    object cdsCabecalhosqlPedidoProduto: TDataSetField
      FieldName = 'sqlPedidoProduto'
    end
  end
